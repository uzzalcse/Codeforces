#include <iostream>
using namespace std;
int main()
{
    int t; cin>>t;
    
    while(t--) {
        int n; cin>>n;
        if(n>=28 && n<=53) {
            char s=n-'z'-'z';
            cout<<'a'<<s<<'z';
        }
        else if(n>53) {
            char s=n-'z';
            cout<<s<<'z'<<'z';
        }
        else {
            char s = n-'a'-'a';
            cout<<'a'<<'a'<<s;
        }
    }

    return 0;
}